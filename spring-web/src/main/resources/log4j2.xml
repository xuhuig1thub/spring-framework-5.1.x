<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="- %msg%d{HH:mm:ss.SSS} [%t] %-5level %logger{1.} %n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="org.springframework.web" level="debug" />
		<Logger name="org.springframework.beans" level="debug" />
		<Logger name="org.springframework.binding" level="debug" />
		<Logger name="org.springframework.http" level="debug" />
		<Logger name="org.springframework.remoting" level="debug" />
		<Root level="debug">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>
